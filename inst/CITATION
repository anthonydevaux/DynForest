citHeader("To cite DynForest in publications use:")

bibentry(
  bibtype  = "Article",
  title    = "Random survival forests with multivariate longitudinal endogenous covariates",
  author   = c(person("Anthony Devaux"), person("Catherine Helmer"), person("Robin Genuer"), person("C\\'ecile Proust-Lima")),
  journal  = "Statistical Methods in Medical Research",
  year     = "2023",
  url      = "https://doi.org/10.1177/09622802231206477"
)

